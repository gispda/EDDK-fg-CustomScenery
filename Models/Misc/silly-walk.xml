<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>silly-walk.ac</path>


<animation>
	<type>alpha-test</type>
	<alpha-factor>0.01</alpha-factor>
</animation>

    <animation>
		<type>textranslate</type>
        <object-name>Plane</object-name>
         <property>/controls/doors/door7/position-norm</property>
      
        <factor>0.1</factor>
     
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>

    </animation>
    
    
    
<animation>
  <type>rotate</type>
    <property>/controls/doors/door7/position-norm</property>
  <factor>2.0</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>20.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
</animation>

    
    
    
<nasal>
	<load>
	var i=10;
	var j=0;
    var update = func(){
      i=i-1;
      j=j+1;
      if (i==0)
       {
			i=9;
       }
		setprop("/controls/doors/door6/position-norm", i)	;
		setprop("/controls/doors/door7/position-norm", j)	;
		timer.restart(0.4); 
	}

	var timer = maketimer(1, update);
    timer.start();
	</load>  
</nasal>	 
    
    
</PropertyList>
