<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>white-cat.ac</path>

    <animation>
		<type>textranslate</type>
        <object-name>Plane</object-name>
         <property>/controls/doors/door8/position-norm</property>
      
        <factor>0.1428</factor>
     
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>

    </animation>
    
    
    
<animation>
  <type>rotate</type>
    <property>/sim/time/elapsed-sec</property>
  <factor>1.0</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>50.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
</animation>

    
    
    
<nasal>
	<load>
		var i=0;
    var update = func(){
     
      if (i==7)
       {
       i=0;
       }
		setprop("/controls/doors/door8/position-norm", i)	;
		 i=i+1;
    timer.restart(0.1); 
}

var timer = maketimer(1, update);
    timer.start();
 </load>  
</nasal>	 
    
    
</PropertyList>
